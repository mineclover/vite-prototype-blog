{
  "name": "react-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "pretest": "npm run lint",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format",
    "prepare": "husky install",
    "marco": "npm run macro --prefix ./docusaurus",
    "setup": "git clone https://github.com/mineclover/my-blog.git docusaurus && npm install --prefix ./docusaurus && npm run setup --prefix docusaurus",
    "upload": "git add . && git commit -m \"$(date '+%Y-%m-%d %H:%M:%S')\" && git push && npm run upload --prefix ./docusaurus"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "axios": "^1.3.4",
    "eslint-import-resolver-typescript": "^3.5.3",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.58.3",
    "sort-by": "^0.0.2",
    "styled-components": "^5.3.8",
    "styled-reset": "^4.4.5",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.3.1",
    "vite-plugin-remark-rehype": "^0.1.0"
  },
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,css,md}": "prettier --write"
  }
}
